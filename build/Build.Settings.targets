<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- Use CustomAfterMicrosoftCSharpTargets property (defined in Microsoft.Common.targets) to
         inject post-Common targets files without requiring the inclusion -->
    <CustomAfterMicrosoftCSharpTargets>$(MSBuildThisFileDirectory)Build.targets</CustomAfterMicrosoftCSharpTargets>

    <!-- Changes to this file should invalidate the MSBuild cache, forcing the next build to be a full build. -->
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>
</Project>
